<%= form_with model: @passenger, class: form_class do |passenger| %>
  
  <div class="form_input">
    <%= passenger.label :name%>
    <%= passenger.text_field :name, placeholder: "Passenger name" %>
  </div>
  
  <div class="form_input">
    <%= passenger.label :phone_num %>
    <%= passenger.text_field :phone_num, placeholder: "1234567890" %>
  </div>
  
  <%= passenger.submit action_name, class: "button" %>
  
<% end %>